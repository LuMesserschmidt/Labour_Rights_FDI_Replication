rownames(PTS.logit.LDV.excl.corr$var)[rownames(PTS.logit.LDV.excl.corr$var)== "Lag_PTS_ai_reversed"] <- "Past"
colnames(PTS.logit.LDV.excl.corr$var)[colnames(PTS.logit.LDV.excl.corr$var)== "Lag_PTS_ai_reversed"] <- "Past"


