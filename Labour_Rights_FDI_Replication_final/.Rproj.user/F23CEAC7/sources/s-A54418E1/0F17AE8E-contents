rownames(PTS.logit.LDV.UN.corr$var)[rownames(PTS.logit.LDV.UN.corr$var)== "Lag_PTS_ai_reversed"] <- "Past"
colnames(PTS.logit.LDV.UN.corr$var)[colnames(PTS.logit.LDV.UN.corr$var)== "Lag_PTS_ai_reversed"] <- "Past"

